{
  "kind": "collectionType",
  "collectionName": "personals",
  "info": {
    "singularName": "personal",
    "pluralName": "personals",
    "displayName": "Персонал",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {
    "i18n": {
      "localized": true
    }
  },
  "attributes": {
    "name": {
      "type": "string",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "required": true,
      "unique": true
    },
    "slug": {
      "type": "uid",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "targetField": "name"
    },
    "image": {
      "type": "media",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images",
        "files"
      ]
    },
    "galery": {
      "type": "media",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "multiple": true,
      "required": false,
      "allowedTypes": [
        "images",
        "files",
        "videos"
      ]
    },
    "description": {
      "type": "customField",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "customField": "plugin::ckeditor5.CKEditor",
      "options": {
        "preset": "defaultHtml"
      }
    },
    "service": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::service.service",
      "inversedBy": "personals"
    },
    "metaData": {
      "type": "component",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "component": "seo.meta",
      "repeatable": false
    },
    "offers": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::offer.offer",
      "mappedBy": "personals"
    },
    "offersDone": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::service-provided.service-provided",
      "mappedBy": "personal"
    },
    "work_time": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::work-time.work-time",
      "mappedBy": "personal"
    },
    "penalties": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::penalty.penalty",
      "mappedBy": "personal"
    },
    "payroll": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::payroll.payroll",
      "mappedBy": "personal"
    },
    "money": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::cash.cash",
      "mappedBy": "personal"
    },
    "advances": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::avans.avans",
      "mappedBy": "personal"
    },
    "position": {
      "type": "enumeration",
      "pluginOptions": {
        "i18n": {
          "localized": false
        }
      },
      "required": true,
      "default": "master",
      "enum": [
        "administrator",
        "master"
      ]
    },
    "rates": {
      "type": "component",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "component": "items.rates",
      "repeatable": true
    }
  }
}
