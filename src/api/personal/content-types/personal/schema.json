{
  "kind": "collectionType",
  "collectionName": "personals",
  "info": {
    "singularName": "personal",
    "pluralName": "personals",
    "displayName": "Персонал",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {
    "i18n": {
      "localized": true
    }
  },
  "attributes": {
    "name": {
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "type": "string",
      "required": true,
      "unique": true
    },
    "slug": {
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "type": "uid",
      "targetField": "name"
    },
    "image": {
      "type": "media",
      "multiple": false,
      "required": true,
      "allowedTypes": [
        "images",
        "files"
      ],
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "galery": {
      "type": "media",
      "multiple": true,
      "required": false,
      "allowedTypes": [
        "images",
        "files",
        "videos"
      ],
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "description": {
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "type": "customField",
      "options": {
        "preset": "default"
      },
      "customField": "plugin::ckeditor5.CKEditor"
    },
    "service": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::service.service",
      "inversedBy": "personals"
    },
    "metaData": {
      "type": "component",
      "repeatable": false,
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "component": "seo.meta"
    },
    "offers": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::offer.offer",
      "mappedBy": "personals"
    },
    "offersDone": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::service-provided.service-provided",
      "mappedBy": "personal"
    },
    "work_time": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::work-time.work-time",
      "mappedBy": "personal"
    },
    "penalties": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::penalty.penalty",
      "mappedBy": "personal"
    }
  }
}
